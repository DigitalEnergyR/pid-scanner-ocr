{
  "extraction_settings": {
    "equipment_tags": {
      "enabled": true,
      "patterns": ["[A-Z]{1,3}-[0-9]{3,4}[A-Z]?", "[A-Z]{2,4}[0-9]{2,4}"]
    },
    "valves": {
      "enabled": true,
      "patterns": ["[A-Z]{1,2}V-[0-9]{3,4}", "V-[0-9]{3,4}", "VALVE-[0-9]{3,4}", "[A-Z]{1,3}V[0-9]{3,4}"]
    },
    "instruments": {
      "enabled": true,
      "patterns": ["[A-Z]{2}-[0-9]{3,4}", "[A-Z]{3}-[0-9]{3,4}", "PT-[0-9]{3,4}", "TT-[0-9]{3,4}", "FT-[0-9]{3,4}", "LT-[0-9]{3,4}", "PI-[0-9]{3,4}", "TI-[0-9]{3,4}", "FI-[0-9]{3,4}", "LI-[0-9]{3,4}"]
    },
    "pipe_specifications": {
      "enabled": true,
      "patterns": ["[0-9]{1,2}\"", "[0-9]{1,2}\\\"", "[0-9]{1,2}\\s*inch", "[0-9]{1,2}\\s*IN"]
    },
    "pressure_ratings": {
      "enabled": true,
      "patterns": ["[0-9]{2,4}\\s*PSI", "[0-9]{2,4}\\s*psi", "[0-9]{1,3}\\s*bar", "Class\\s*[0-9]{3,4}"]
    },
    "material_specifications": {
      "enabled": true,
      "patterns": ["SS316", "CS", "A106", "A53", "ASTM\\s*[A-Z][0-9]{2,3}"]
    }
  },
  "ocr_settings": {
    "dpi": 300,
    "preprocessing": {
      "denoise": true,
      "enhance_contrast": true,
      "sharpen": true
    },
    "tesseract_config": "--psm 6 -c tessedit_char_whitelist=0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz-./\"' "
  },
  "output_format": "json"
}